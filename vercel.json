{
  "outputDirectory": "dist",
  "buildCommand": null,
  "installCommand": null,
  "framework": null,
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/admin/leads", "destination": "/admin/leads.html" },
    { "source": "/admin/board", "destination": "/admin/board.html" },
    { "source": "/admin/analytics", "destination": "/admin/analytics.html" },
    { "source": "/admin/settings", "destination": "/admin/settings.html" }
  ],
  "headers": [
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
